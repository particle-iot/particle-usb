{
  "name": "particle-usb",
  "version": "0.4.0",
  "description": "A library for accessing Particle USB devices",
  "main": "lib/particle-usb.js",
  "keywords": [
    "particle",
    "usb"
  ],
  "browser": {
    "./src/usb-device-node.js": "./src/usb-device-webusb.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spark/particle-usb"
  },
  "license": "Apache-2.0",
  "contributors": [
    "Sergey Polyakov <sergeuz@gmail.com>",
    "Andrey Tolstoy <avtolstoy@gmail.com>"
  ],
  "scripts": {
    "prepublish": "git submodule update --init && npm run compile",
    "version": "npm run docs && git add docs",
    "compile": "npm run compile-babel && npm run compile-proto",
    "compile-proto": "pbjs proto/control/*.proto -t static-module -o lib/pb-message.js --no-beautify --no-comments --no-delimited --no-convert --no-verify",
    "compile-babel": "babel src -s -d lib",
    "docs": "documentation build src/** -g -f md -o docs/reference.md",
    "test": "mocha -r babel-register -r babel-polyfill test test/integration",
    "webpack": "webpack-cli"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "protobufjs": "^6.8.3",
    "usb": "https://github.com/particle-iot/node-usb/archive/particle-1.5.0.tar.gz",
    "verror": "^1.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "documentation": "^5.3.5",
    "mocha": "^4.0.1",
    "mocha-sinon": "^2.0.0",
    "sinon": "^4.1.2",
    "sinon-chai": "^2.14.0",
    "webpack": "^4.29.1",
    "webpack-cli": "^3.2.3"
  }
}
